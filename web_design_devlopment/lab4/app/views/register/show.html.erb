<table>
  <tr>
    <th>id</th>
    <th>Username</th>
    <th>Gender</th>
    <th>Reference</th>
    <th>Subscribe</th>
  </tr>
  <% @registrations.each do |reg| %>
  <tr>
    <td><%= reg.id %></td>
    <td><%= reg.username %></td>
    <td><%= reg.gender %></td>
    <td><%= reg.reference %></td>
    <td><%= reg.subscribe %></td>
    <td><%= link_to :delete, "/delete_registration/#{reg.id}", data: {confirm: 'Are you sure?'}, method: :delete %></td>
  </tr>
  <% end %>
</table>